# Frontend Environment Variables

# API Configuration
REACT_APP_API_URL=http://localhost:8000/api/v1
REACT_APP_ENVIRONMENT=development

# App Configuration
REACT_APP_NAME=FinanceAI
REACT_APP_VERSION=1.0.0

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_UPI=true
REACT_APP_ENABLE_AI_CHAT=true

# External Services (Optional)
REACT_APP_GOOGLE_ANALYTICS_ID=your-ga-id-here
REACT_APP_SENTRY_DSN=your-sentry-dsn-here

# Theme Configuration
REACT_APP_THEME_PRIMARY_COLOR=#667eea
REACT_APP_THEME_SECONDARY_COLOR=#764ba2